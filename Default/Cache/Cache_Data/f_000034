	<script>
	$(function() {	
		
 		(function ($) {
 		    $.fn.initAddressChk = function() {
		    		
		    	function toggleStates(e){
		    		showStates('registeredAgent_address');
		    		return false;
		    	} 
		    	
		    	populateStates();
		    	var selectCountryBox = '#registeredAgent_address_country';
		    	$(selectCountryBox + " option[value='blank']").remove();
		    	initStates('registeredAgent_address','OR','USA','OR');
		    	$('#registeredAgent_address_country').change(toggleStates);	
 		    	
 		    	$('#registeredAgent_address_addressLine1').on('keyup',addrKeyup);
 		    	$('#registeredAgent_address_city').on('keyup',addrFocusOut);
 		    	$('#registeredAgent_address_zip').on('keyup',addrFocusOut);
 		    	$('#registeredAgent_address_zip').on('focusout', function() {
 		    			$(this).trigger("change");	
 		    	});
 		    	function addrKeyup(e){
 		    		var addressLine1ValLength = $('#registeredAgent_address_addressLine1').val().length;
 		    		if (addressLine1ValLength > 6){
 		    			return addrFocusOut(e);
 		    		}
 		    	    return false;
 		    	}
 		    	
 		    	function addrFocusOut(e){
 		    		var countryVal = $('#registeredAgent_address_country').val();
 		    		if (countryVal == 'USA'){
 		    			validateAddressOnServer(e, 'registeredAgent_address', 'registeredAgent_address','true', 'OR');
 		    			//e.currentTarget.focus();
 		    		}
 		    	    return false;
 		    	}
 		    	
		    	$('#registeredAgent_address'+'ClearAddressHrefId').click(clearClicked);
		    	function clearClicked(e){
 		    		$('#registeredAgent_address_addressLine1').val('');
 		    		$('#registeredAgent_address_addressLine2').val('');
 		    		$('#registeredAgent_address_city').val('');
 		    		$('#registeredAgent_address_zip').val(''); 
 		    		showStates('registeredAgent_address', 'USA', 'OR');
 		    		$('#registeredAgent_address_extrnAddressListResult').val('0');
	 		    	$('#registeredAgent_address_addressLine1').focus();
	 		    	$('#registeredAgent_address_addressLine1').trigger('change');
		    		return false;
		    	} 
		    	
		    	registeredAgentSectionObj.addIgnoredField('registeredAgent_address_altAddr_addressLine1');
		    	registeredAgentSectionObj.addIgnoredField('registeredAgent_address_altAddr_addressLine2');
		    	registeredAgentSectionObj.addIgnoredField('registeredAgent_address_altAddr_city');
		    	registeredAgentSectionObj.addIgnoredField('registeredAgent_address_altAddr_state');
		    	registeredAgentSectionObj.addIgnoredField('registeredAgent_address_altAddr_zip');
		    	registeredAgentSectionObj.addIgnoredField('registeredAgent_address_altAddr_country');
		    	registeredAgentSectionObj.addIgnoredField('registeredAgent_address_altAddr_errorcode');
		    	registeredAgentSectionObj.addIgnoredField('registeredAgent_address_altAddr_errormessage');
 		    }
		 })(jQuery);
		 $('#registeredAgent_address').initAddressChk(); 		 
 	});
	</script>
