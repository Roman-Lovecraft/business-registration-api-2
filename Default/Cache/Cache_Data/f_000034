<script src="https://secure.sos.state.or.us/cbrstatic/js/preferences.js?timestamp=0307022025"
	type="text/javascript"></script>
<script type="text/javascript">
	var notifyDiv = $('<div class="standardDataDisplayStyle" />');
	notifyDiv.text('For future filings, would you like to auto-fill the Daytime Contact with this data? Use User Preferences (Help Menu) to change this in the future.');
	busOverviewSectionObj.addReplacementValue('principalAddr_','principalAddress.');
	busOverviewSectionObj.addIgnoredField('busOverview_principalAddr_extrnAddressListResult');
	busOverviewSectionObj.onSuccessUpdateMethod = function(jsonObj) {
		if(jsonObj != undefined && jsonObj.entityRsn != undefined) {
			var exitButton = $('#pageExitButton');
			var exitButtonHref = exitButton.prop('href');
			var pos = exitButtonHref.indexOf(jsonObj.entityRsn.toString());
			if(pos == -1){
				exitButton.prop('href', exitButtonHref + jsonObj.entityRsn);
			}
		}
		setupAutoPrefil('busOverview',busOverviewSectionObj, 'PREFILL');
		this.businessOverviewAutofill(jsonObj, this);
	};
	busOverviewSectionObj.requiredFieldId = 'busOverview_businessName';
	busOverviewSectionObj.requiredSection = true;

	$('#busOverview_businessName').keyup(businessNameLengthCheck);
	$('#busOverview_businessName').change(function() {
		var field = $(this);
		field.val(field.val().toUpperCase());
	});
    var businessNameCheckUrl = 'http://egov.sos.state.or.us/br/pkg_web_name_srch_inq.do_name_srch?p_regist_nbr=&p_srch=NAC&p_print=FALSE&p_entity_status=ACTINA&p_name=';
	$('#busOverview_busName_chkName').click(function(){
		var businessName=$('#busOverview_businessName').val();
		
		// if the business name is blank then the call to check the business name will fail.
		if(businessName.trim() == '') {
			// show error message to user
			$('<div/>').text('You must supply a business name before you can check its availability.').createModalOkDialog('blankBusNameCheckAvailMessageDiv','Blank Business Name',null,450,150);
		} else {
			var encodedBusinessName = encodeURIComponent(businessName);
			window.open(businessNameCheckUrl+encodedBusinessName);
		}
		
	});
</script>



<!--end businessOverview.jspf -->

			 

			 
			





