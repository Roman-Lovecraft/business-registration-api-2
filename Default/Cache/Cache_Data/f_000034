	<script>
	$(function() {	
		
 		(function ($) {
 		    $.fn.initAddressChk = function() {
		    		
		    	function toggleStates(e){
		    		showStates('organizer_address');
		    		return false;
		    	} 
		    	
		    	populateStates();
		    	var selectCountryBox = '#organizer_address_country';
		    	$(selectCountryBox + " option[value='blank']").remove();
		    	initStates('organizer_address','INT','USA','OR');
		    	$('#organizer_address_country').change(toggleStates);	
 		    	
 		    	$('#organizer_address_addressLine1').on('keyup',addrKeyup);
 		    	$('#organizer_address_city').on('keyup',addrFocusOut);
 		    	$('#organizer_address_zip').on('keyup',addrFocusOut);
 		    	$('#organizer_address_zip').on('focusout', function() {
 		    			$(this).trigger("change");	
 		    	});
 		    	function addrKeyup(e){
 		    		var addressLine1ValLength = $('#organizer_address_addressLine1').val().length;
 		    		if (addressLine1ValLength > 6){
 		    			return addrFocusOut(e);
 		    		}
 		    	    return false;
 		    	}
 		    	
 		    	function addrFocusOut(e){
 		    		var countryVal = $('#organizer_address_country').val();
 		    		if (countryVal == 'USA'){
 		    			validateAddressOnServer(e, 'organizer_address', 'organizer_address','true', 'INT');
 		    			//e.currentTarget.focus();
 		    		}
 		    	    return false;
 		    	}
 		    	
		    	$('#organizer_address'+'ClearAddressHrefId').click(clearClicked);
		    	function clearClicked(e){
 		    		$('#organizer_address_addressLine1').val('');
 		    		$('#organizer_address_addressLine2').val('');
 		    		$('#organizer_address_city').val('');
 		    		$('#organizer_address_zip').val(''); 
 		    		showStates('organizer_address', 'USA', 'OR');
 		    		$('#organizer_address_extrnAddressListResult').val('0');
	 		    	$('#organizer_address_addressLine1').focus();
	 		    	$('#organizer_address_addressLine1').trigger('change');
		    		return false;
		    	} 
		    	
		    	organizerSectionObj.addIgnoredField('organizer_address_altAddr_addressLine1');
		    	organizerSectionObj.addIgnoredField('organizer_address_altAddr_addressLine2');
		    	organizerSectionObj.addIgnoredField('organizer_address_altAddr_city');
		    	organizerSectionObj.addIgnoredField('organizer_address_altAddr_state');
		    	organizerSectionObj.addIgnoredField('organizer_address_altAddr_zip');
		    	organizerSectionObj.addIgnoredField('organizer_address_altAddr_country');
		    	organizerSectionObj.addIgnoredField('organizer_address_altAddr_errorcode');
		    	organizerSectionObj.addIgnoredField('organizer_address_altAddr_errormessage');
 		    }
		 })(jQuery);
		 $('#organizer_address').initAddressChk(); 		 
 	});
