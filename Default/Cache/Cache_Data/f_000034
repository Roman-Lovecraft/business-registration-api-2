<script type="text/javascript">
    var benefitCompanyConfirmDialogVar;
    
	provisionsSectionObj.addIgnoredField('provisions_indemnificationSelected');
	provisionsSectionObj.addIgnoredField('provisions_benefitSelected');

	provisionsSectionObj.requiredFieldId = 'provisions_reviewed';

	if('false'=='true') {
		$('#provisions_indemnificationSelected').prop('checked', true);
	}
	if('false'=='true') {
		$('#provisions_benefitSelected').prop('checked', true);
	}
	provisionsSectionObj.sectionOnOpenMethod = function() {
		var reviewedInput = $('#provisions_reviewed');
		if(reviewedInput.val() == '') {
			reviewedInput.val(true).change();
		}
	}
	// redefine the update method so that it only fires if the section has been reviewed
	provisionsSectionObj.update = function() {
		if(this.dirty) {
			// reset the dirty indicator
			this.dirty = false;
		}

		// verify that minimum requiredFields have values
		var jsonData = this.getDataAsJSON();
		if(this.debug){alert("jsonData: " + jsonData);}

		// only run the update method if the section has been reviewed
		if($('#provisions_reviewed').val()) {
			var myCbrPageObj = this.getCbrPageObject();
			var myObj = this;
			$.ajax({
			    type: 'POST',
			    url: this.updateUrl,
			    data: jsonData,
				async : false,
				beforeSend: function() {
					// if a page level ajax call is NOT in progress then block the UI
					if(!myObj.getCbrPageObject().pageLevelAjaxInProgress) {
						blockUI();
					}
				},
			    success: function(jsonData) {
			    	myObj.successFunction(myObj,jsonData);
					// if a page level ajax call is NOT in progress then unblock the UI
					if(!myObj.getCbrPageObject().pageLevelAjaxInProgress) {
						unblockUI();
					}
				}
			});
		}
	}
	var provisionFieldId = $('#provisions_optionalProvision_description');
	var PROVISION_MAX_LENGTH = 4000;

	function updateCountdownProvisions() {
	    var remaining = PROVISION_MAX_LENGTH - $(provisionFieldId).val().length;
	    $('#countdownProvisions').text(remaining);
	}
	
	function handleIndemnificationChange(event) {
		if (this.checked) {
			$('#provisions_indemnification').val("true");
		} else {
			$('#provisions_indemnification').val("false");
		}
	}
	function handleBenefitChange(event) {
		if (this.checked) {
			benefitCompanyConfirmDialogVar.dialog("open");
		} else {
			$('#provisions_benefit').val("false");
		}
	}
	
	$(function($) {
		benefitCompanyConfirmDialogVar = $("#benefitCompanyConfirmDialog").dialog({
	        modal: true,
	        dialogClass: 'no-close',
	        autoOpen: false,
	        title: "Confirm/Cancel Benefit Company Selection",
	        width: responseWidth,
	        maxHeight: 650,
	        buttons: {
	        	"Confirm": function(){
	        						$('#provisions_benefit').val("true");
	        						benefitCompanyConfirmDialogVar.dialog("close");
	        						},
	        	Cancel: function(){
	        						$('#provisions_benefitSelected').prop('checked', false);
	        						$('#provisions_benefit').val("false");
	        	                    benefitCompanyConfirmDialogVar.dialog("close");
	        	                   }
	        }
	    });
		updateCountdownProvisions();
	    $(provisionFieldId).on('input', updateCountdownProvisions);
	    $('#provisions_indemnificationSelected').change(handleIndemnificationChange);
	    $('#provisions_benefitSelected').change(handleBenefitChange);
	});
</script>
			 
			 

		</div>
		
		
	
	<div id="buttonBarDiv" class="mt-5 displayNone"></div>
</div>

