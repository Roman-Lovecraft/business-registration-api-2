	<script type="text/javascript">
	$(function() {
		
		// itialize the CbrPage object
		cbrPageObj.initialize();
		
		if((cbrPageObj.activityType === 'oic') && (cbrPageObj.firstTimeLoadingNewOicPageIndicator === true))
	    {
			$("#pageErrorDiv").hide();
			if ($("#errorPop").hasClass('ui-dialog-content')) {
				 $( "#errorPop" ).dialog( "close" );
			}
		}

		// unblock the user and allow them to use the page
		unblockUI();
		
	});
</script>		
