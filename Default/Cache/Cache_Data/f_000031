<script>
						

function doRedirect() {
	var binConfirm = $('<div class="standardDataDisplayStyle smallText" />');
//	$("#externalLinkModal").removeClass("d-none");
	var externalLinkModal = $("#externalLinkModal").clone();
	$(externalLinkModal).removeClass("d-none");
	binConfirm.append(externalLinkModal);	
	
	$(".ui-dialog-content").dialog("close");	

		var binInfoPopUp = binConfirm.createModalDialog('binConfirmDialog','You are about to be transferred to another site...', 
		[ { text: "Continue", id: "yesButton", 
			click: function() { 
				blockUI();
				var href = 'https://revenueonline.dor.oregon.gov/tap/_/?link=BIN'
				window.location.href=href;
				// close and remove the dialog
                $(this).remove(); 
				} 
			},
		  { text: "Cancel", id: "noButton", 
			click: function() { 
						$(this).remove(); 
					} 
			}],null, responseWidth, 300);
}
</script>


