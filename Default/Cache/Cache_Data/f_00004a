<script>

//  closes the help dropdown when preferences or contact us is clicked
	$('#userPreferencesMenu, #contactDialogMenu').click(function() {
		$("#helpDropMenu").collapse('hide');
	});

	var menuBarIndex = 2000;
	$('div#menu').find('a').each(function() {
		$(this).attr('tabindex', menuBarIndex++);
	});

	
	$(function() {
		initializeFullScreen('fullScreenHref', 'false');
    });
	
</script>

	</div>

	<!-- TITLE BAR -->
	
		<div id="titleBar">
			<div class="container-fluid pl-3 pl-md-5 mt-4">

				<div class="row">
					<div id="titleBarValue" class="col-12 col-sm-7 largeText">
						
						
							
							OREGON TEST LLC
						
					</div>

					
					
					<!-- VALIDATION NOTES OPENER -->
				<div id="pageErrorOpenerDiv" class="displayNone col-12 col-sm-4 mt-2 mt-sm-0">
				<button id="pageErrorOpenerButton" class="float-sm-right">Validation Notes</button>
				</div>
	
				</div>
