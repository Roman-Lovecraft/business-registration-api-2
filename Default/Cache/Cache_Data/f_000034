<script type="text/javascript">
	memberManagerSectionObj.saving = false;
	memberManagerSectionObj.addIgnoredField('memberManager_memberManagerAssocNameTypeMember');
	memberManagerSectionObj.addIgnoredField('memberManager_memberManagerAssocNameTypeManager');
	memberManagerSectionObj.multiObjectAddId = 'memberManager_multiObjectAdd';
	memberManagerSectionObj.multiObjectTableId = 'memberManager_multiAssocNameDisplayTable';
	memberManagerSectionObj.defaultAssocNameType = 'MEM,MGR';
	
	$('#memberManager_addMemberManagersYes').bind("click", {parentObject:memberManagerSectionObj}, memberManagerSectionObj.addMultiObject);
	
	$('#memberManager_multiObjectAdd').on("click", function(){$('#memberManager_addEditEntityDiv').clearValues();});

	var cancelPopupFunction = function() {
		$(this).dialog('close');
		return false;
	}
	var resetPopupFunction = function() {
		//$(this).dialog('close');
		$('#memberManager_addEditEntityDiv').clearValues();
		$('#memberManager_assocNameType').val('');
		// if there are no entries then set the addMemberManagers radio button to 'No' and set the validity of the section back to what it was previously, then run the section method to set the show/hide the checkmark
		if(memberManagerSectionObj.multipleObjectsArray.length == 0) {
			$('input:radio[name="addMemberManagers"][value="false"]').click();	
			memberManagerSectionObj.valid = previousValidState;
			memberManagerSectionObj.setSectionCssClass();
		} else if(!memberManagerSectionObj.saving){
			// if there were entries then use the section method for restoring the previous state
			memberManagerSectionObj.restoreMultiObjectToPreviousState();
		}
		memberManagerSectionObj.saving = false;
		$('#memberManager_editExistingName').val(false);
		return false;
	};
	
	//set width of popup based on window size
// 				var popUpWidth;	
// 				function setPopUp(){
// 					if (window.innerWidth > 991){
// 						popUpWidth = '40vw';
// 					} else if (window.innerWidth > 767) {
// 						popUpWidth = '60vw';
// 					} else {
// 						popUpWidth = '95vw';
// 					}
// 				}	
// 				setPopUp();

 
	memberManagerSectionObj.multiObjectPopupDiv = $('#memberManager_addEditEntityDiv')
	.createModalDialog(memberManagerSectionObj.uniqueSectionId,'Add/Edit Member/Manager', 
		[ { text: "Save", id: 'memberManager'+"_saveButton", 
			click: function() { 
				$('#memberManager_editExistingName').val(true);
				memberManagerSectionObj.getCbrPageObject().currentMultiObjAddEditPopupSaveButtonID = 'memberManager'+'_saveButton';
				memberManagerSectionObj.getCbrPageObject().currentMultiObjAddEditPopupCancelButtonID = 'memberManager'+'_cancelButton';
				memberManagerSectionObj.wasMultiObjUpdateDueToSaveButton = true;
				memberManagerSectionObj.update();
				// if the  multi object that is currently being edited is valid after updating, close this dialog box
				var occurrence = $('#memberManager_assocNameOccurrence').val();
				var multiObject = memberManagerSectionObj.multipleObjectsArray[occurrence];
				if(multiObject != undefined && multiObject.valid) {
					memberManagerSectionObj.saving = true;
					memberManager = $('#memberManagerErrorDiv');
					if(memberManager != null) {
						memberManager.hide();
					}
					$(this).dialog('close');
					$('#memberManager_addEditEntityDiv').clearValues();
					memberManagerMultiTable.removeClass('displayNone');
					addMemberManagersQuestionDiv.addClass('displayNone');
				}
			}
		},
		{ text: "Cancel", id: 'memberManager'+"_cancelButton", 
			click: cancelPopupFunction
		  }		  
    
		],null, responseWidth, 400 , false, resetPopupFunction);

	

	memberManagerSectionObj.addReplacementValue('addMemberManagersValue','addMemberManagers');	// change this to match the expected var name in the bean
	memberManagerSectionObj.addReplacementValue('memberManager_address_','address.');
	memberManagerSectionObj.addIgnoredField('memberManager_address_extrnAddressListResult');
	memberManagerSectionObj.addIgnoredField('memberManager_address_satoriResult');
	memberManagerSectionObj.requiredFieldId = 'memberManager_addMemberManagersYes';

	memberManagerSectionObj.changeAssocNameSecondaryValues = function(assocName) {
		// this method is called even for new member/managers, so check that this isn't a new one
		if(assocName.assocNameOccurrence > -1) {
	 		$('input:radio[name="memberManagerAssocNameType"][value="' + assocName.assocNameType + '"]').click();
		}
	}
	memberManagerSectionObj.onSuccessPostDeleteMethod = function() {
		// check to see if there are any remaining member managers
		// if there are none left, hide the multiObjectTable and show the initial question with "no" selected
		if(memberManagerSectionObj.multipleObjectsArray.length == 0) {
			memberManagerMultiTable.addClass('displayNone');
			addMemberManagersQuestionDiv.removeClass('displayNone');
			
			//Force validation if all deleted.
			if (this.multipleObjectsArray.length == 0) {
				$('input:radio[name=memberManagerAssocNameType]').each(function(){
					$(this).attr('checked',false);
				});
				$('input:radio[name=memberManager_assocNameEntityType]').each(function(){
					$(this).attr('checked',false);
				});
				this.valid = false;
				this.dirty = true;
				memberManagerSectionObj.memMgrSectionHasZeroEntriesAfterDelete = true;
				$('#'+memberManagerSectionObj.uniqueSectionId+'_assocNameOccurrence').val('0');
				$('#'+memberManagerSectionObj.uniqueSectionId+'_assocNameEntityType').val('');
				$('#'+memberManagerSectionObj.uniqueSectionId+'_assocNameType').val('');
				$('#'+memberManagerSectionObj.uniqueSectionId+'EntityAddressFieldsId').addClass('displayNone');
				$('input:radio[name="addMemberManagers"][value="false"]').click();
				$('#pageButton2').click();
			}
			//$('input:radio[name="addMemberManagers"][value="false"]').click();
		} 
	} 
	
	var addMemberManagersQuestionDiv = $('#addMemberManagersQuestionId');
	var memberManagerMultiTable = $('#memberManagerMultiTable');
	
	// set the value for the radio button and the form value if it comes out of the 
	var memberManagersIncluded = '';
	if(memberManagersIncluded != '') {
		$('input:radio[name="addMemberManagers"][value="' + memberManagersIncluded + '"]').attr('checked',true);
		$('#addMemberManagersValue').val(memberManagersIncluded);
		addMemberManagersQuestionDiv.removeClass('requiredFieldPromptStyle').addClass('standardFieldPromptStyle');
	}
		
	// if there are already member/managers entered:
	// 1 - hide the initial question 
	// 2 - show the table displaying member/managers
	if(memberManagerSectionObj.multipleObjectsArray.length > 0) {
		memberManagerMultiTable.removeClass('displayNone');
		addMemberManagersQuestionDiv.addClass('displayNone');
	} 
	
	// 	if a memberManagerAssocNameType radio button is clicked then:
	//		1 - set the hidden assocNameType field 
	//		2 - change the css class so that the prompt text isn't bold
	$('input:radio[name=memberManagerAssocNameType]').each(function(){
		$(this).click(function(){
			$('#memberManagerChoiceQuestionId').removeClass('requiredFieldPromptStyle').addClass('standardFieldPromptStyle');
			$('#memberManager_assocNameType').val($(this).val());
		});
	});
	
	$('input:radio[name=addMemberManagers]').each(function(){
		$(this).click(function(){
			$('#addMemberManagersValue').val($(this).val());
			addMemberManagersQuestionDiv.removeClass('requiredFieldPromptStyle').addClass('standardFieldPromptStyle');
		});
	});
	
	var previousValidState = '';
	$('#memberManager_addMemberManagersYes').change(function(){
		// save the current validity state of the sectionObj
		previousValidState = memberManagerSectionObj.valid;
	});
	$("#memberManager_addEditEntityDiv input[type='text']").change(function() {
		handleSpecialCharOnInput(this);
	});
	
	$(function() {
		//Attach to all buttons
		$('#editButtonBarDiv a').on("click", function() {
			//Give section time to get validated.
			setTimeout(function() {
				var multiObject = memberManagerSectionObj; 
				if(multiObject != undefined && multiObject.valid) {
					sectionErrorDiv = memberManagerSectionObj.sectionError.getSectionErrorDiv();
					if(sectionErrorDiv && sectionErrorDiv.length != 0) {
						sectionErrorDiv.remove();
					}
				}
			}, 2000);
		});
	});
</script>





			
			
			

			
















 



