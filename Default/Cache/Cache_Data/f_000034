<script src="https://secure.sos.state.or.us/cbrstatic/js/preferences.js?timestamp=0307022025" type="text/javascript"></script>
<script type="text/javascript">

$(function () {
	
	if( $("#eSelection" ).val() == "PAPER" || $("#eSelection" ).val() == "default"){	
		 $("#showDetails").hide();
	 }	else {
	 	 $("#showDetails").show();
	 }

	 $("#eSelection").change(function () {
        if (($(this).val() == "EMAIL") || ($(this).val() == "EN-PRINT")) {
            $("#showDetails").show();
        } else {
            $("#showDetails").hide();
        }
    });
	 
	 
	   $( "#eNoticedialog" ).dialog({
            autoOpen: false, 
	            modal: true,
	            title: "Electronic Notice Method Selection",
	            responsive: true,
	            buttons: {
	               Cancel: function() {$(this).dialog("close");}
	            },
	         });
	        
	         $( "#eNoticeDialogOpener" ).click(function() {
	            $( "#eNoticedialog" ).dialog( "open" );
	         });
	    });
</script>
			 
			
			



 














 



