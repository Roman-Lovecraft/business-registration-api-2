<script type="text/javascript">
	var indDirectKnowledgeotherDivsToShowHideIdArray = new Array();
	
	indDirectKnowledgeotherDivsToShowHideIdArray = 'indDirectKnowledge_addressDiv'.split(',');
	
	
	// ignoredFields
	indDirectKnowledgeSectionObj.addIgnoredField('_extrnAddressListResult');
	indDirectKnowledgeSectionObj.addIgnoredField('indDirectKnowledge_indvAssocNameEntityType');
	indDirectKnowledgeSectionObj.addIgnoredField('indDirectKnowledge_bofrAssocNameEntityType');
	indDirectKnowledgeSectionObj.addIgnoredField('indDirectKnowledge_bnofrAssocNameEntityType');

	indDirectKnowledgeSectionObj.defaultAssocNameType = 'IDK';

	// if the assocNameType is not set, then set it to the default
	var my_indDirectKnowledge_assocNameType = $('#indDirectKnowledge_assocNameType');
	if(my_indDirectKnowledge_assocNameType.val() == '') {
		my_indDirectKnowledge_assocNameType.val(indDirectKnowledgeSectionObj.defaultAssocNameType);
	}
	
	// boolean to track if the registryNumber changes
	// TODO: Is this global variable really needed?  
	// Why can't we assign the function in initializeAssocNameEntityTypeSection
	// to the onchange method instead?
	var indDirectKnowledge_businessOfRecord_registryNumber_changed = false;
	
	$(function() {
		
		initializeAssocNameEntityTypeSection('indDirectKnowledge', '', indDirectKnowledge_businessOfRecord_registryNumber_changed, false,indDirectKnowledgeotherDivsToShowHideIdArray, true);
	});
