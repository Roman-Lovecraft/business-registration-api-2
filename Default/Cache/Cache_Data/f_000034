<script type="text/javascript">


	
	// add the onclick event for the continue button
	$(function() {
		// set the entity type
		$('#filingType').val(cbrPageObj.entityType);
		if (false) {
			$('#filingType').prop('disabled','disabled');
		}
		$('#filingType').change(function(event){		
			//adjust width of dialog based on screen width
			var wWidth = $(window).width();
			var responseWidth;
			if (wWidth > 800){
			 	responseWidth = '30%'
			} else {
			 	responseWidth = '80%'
			}
			var filingTypeDropdown = $(this);
			var filingType = filingTypeDropdown.val();
			// confirm that the user wants to do this
			var confirmChangeBusinessTypeDiv = $('<div class="standardDataDisplayStyle" />');
			confirmChangeBusinessTypeDiv.html('Changing a business entity type may result in a loss of previously entered data.<br/><br/><div class="emphasis">Please review all previously entered data prior to submission.</div>');
			confirmChangeBusinessTypeDiv.createModalDialog('confirmChangeBusinessTypeModalDialog','Change Business Entity Type',
				[{ 
					text: "Change Entity Type", id: "changeBusinessTypeButton", 
					click: function() { 
						window.location = 'https://secure.sos.state.or.us/cbrbr/translateBrRegistration.action?filingType=' + filingType;
					}
				},
				{
					text: "Cancel Change", id: "cancelChangeBusinessTypeButton",
					click: function() {
						$(this).remove(); 
						filingTypeDropdown.val(cbrPageObj.entityType);
					}
				}],
			null, responseWidth, 400, null, 
			function(event,ui) {
				filingTypeDropdown.val(cbrPageObj.entityType);
			});
		});
	});
</script>


			 
			




