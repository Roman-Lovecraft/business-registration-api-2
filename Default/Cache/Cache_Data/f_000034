	<!-- the EDIT entry address format -->
	<script>
	$(function() {	
		
 		(function ($) {
 		    $.fn.initAddressChk = function() {
		    		
		    	function toggleStates(e){
		    		showStates('indDirectKnowledge_address');
		    		return false;
		    	} 
		    	
		    	populateStates();
		    	var selectCountryBox = '#indDirectKnowledge_address_country';
		    	$(selectCountryBox + " option[value='blank']").remove();
		    	initStates('indDirectKnowledge_address','INT','USA','OR');
		    	$('#indDirectKnowledge_address_country').change(toggleStates);	
 		    	
 		    	$('#indDirectKnowledge_address_addressLine1').on('keyup',addrKeyup);
 		    	$('#indDirectKnowledge_address_city').on('keyup',addrFocusOut);
 		    	$('#indDirectKnowledge_address_zip').on('keyup',addrFocusOut);
 		    	$('#indDirectKnowledge_address_zip').on('focusout', function() {
 		    			$(this).trigger("change");	
 		    	});
 		    	function addrKeyup(e){
 		    		var addressLine1ValLength = $('#indDirectKnowledge_address_addressLine1').val().length;
 		    		if (addressLine1ValLength > 6){
 		    			return addrFocusOut(e);
 		    		}
 		    	    return false;
 		    	}
 		    	
 		    	function addrFocusOut(e){
 		    		var countryVal = $('#indDirectKnowledge_address_country').val();
 		    		if (countryVal == 'USA'){
 		    			validateAddressOnServer(e, 'indDirectKnowledge_address', 'indDirectKnowledge_address','true', 'INT');
 		    			//e.currentTarget.focus();
 		    		}
 		    	    return false;
 		    	}
 		    	
		    	$('#indDirectKnowledge_address'+'ClearAddressHrefId').click(clearClicked);
		    	function clearClicked(e){
 		    		$('#indDirectKnowledge_address_addressLine1').val('');
 		    		$('#indDirectKnowledge_address_addressLine2').val('');
 		    		$('#indDirectKnowledge_address_city').val('');
 		    		$('#indDirectKnowledge_address_zip').val(''); 
 		    		showStates('indDirectKnowledge_address', 'USA', 'OR');
 		    		$('#indDirectKnowledge_address_extrnAddressListResult').val('0');
	 		    	$('#indDirectKnowledge_address_addressLine1').focus();
	 		    	$('#indDirectKnowledge_address_addressLine1').trigger('change');
		    		return false;
		    	} 
		    	
		    	indDirectKnowledgeSectionObj.addIgnoredField('indDirectKnowledge_address_altAddr_addressLine1');
		    	indDirectKnowledgeSectionObj.addIgnoredField('indDirectKnowledge_address_altAddr_addressLine2');
		    	indDirectKnowledgeSectionObj.addIgnoredField('indDirectKnowledge_address_altAddr_city');
		    	indDirectKnowledgeSectionObj.addIgnoredField('indDirectKnowledge_address_altAddr_state');
		    	indDirectKnowledgeSectionObj.addIgnoredField('indDirectKnowledge_address_altAddr_zip');
		    	indDirectKnowledgeSectionObj.addIgnoredField('indDirectKnowledge_address_altAddr_country');
		    	indDirectKnowledgeSectionObj.addIgnoredField('indDirectKnowledge_address_altAddr_errorcode');
		    	indDirectKnowledgeSectionObj.addIgnoredField('indDirectKnowledge_address_altAddr_errormessage');
 		    }
		 })(jQuery);
		 $('#indDirectKnowledge_address').initAddressChk(); 		 
 	});
	</script>
