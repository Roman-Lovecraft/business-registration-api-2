<script type="text/javascript">

	//adjust width of dialog based on screen width
	var wWidth = $(window).width();
	var responseWidth;
	if (wWidth > 800){
	 	responseWidth = '30%'
	} else {
	 	responseWidth = '80%'
	}

	$(function(){
		
			initializeSessionTimeout(60,$('#timeoutDialog'),$('#timeoutWarning'),$('#timeoutNotice'),$("#extendSessionForm"));
			$('#timeoutDialog').dialog({close: function(event, ui){
				var title = $(this).dialog("option","title");
				if(title=='Timeout Notice') {
					setTimeout(function() {window.location.replace('https://secure.sos.state.or.us:443/cbrmanager/index.action');}, 500); 
				}
			},
			modal: true
			});
			try {
				$('#timeoutDialog').dialog("close");
			} catch(e) {}
				
			
		// loads and opens contact us modal						
		$(".contactDialogOpener").click(function(){
			$('#contactDialog').load('contact.action', function() {
			    $(this).dialog({
			        modal: true,
			        //autoOpen: false,
			        title: "Send Email",
			        width: responseWidth
			    });
			});
			//$('#contactDialog').dialog("open");
			return false;
		});
				
				
		// loads and opens preferences modal						
		$("#userPreferencesMenu").click(function(){
			$('#preferenceDialog').load('preference.action', function() {
			    $(this).dialog({
			        modal: true,
			        title: "User Preferences",
			         width: responseWidth
			    });
			});
			return false;
		});
			
		//opens about modal
		var aboutPopupDialogVar = $('#aboutPopupDiv').removeClass("displayNone").createModalOkDialog('aboutPopupDivDialog', 'About', null, 500, 420, true, false);
		$('.aboutPopupDialogOpener').click(function() {
			aboutPopupDialogVar.dialog('open');
		});
		
		// find any anchor tags with blockUIOnClick class and add the blockUI call when clicked
		$('a.blockUIOnClick').each(function(){
			$(this).click(function(){
				blockUI(); 
			});
		});
		var headerActionIndex = 3000;
		$('#banner-login').find('a').each(function(){
			$(this).attr('tabindex',headerActionIndex++);
		});
		var footerActionIndex = 4000;
		$('#footer').find('a').each(function(){
			$(this).attr('tabindex',footerActionIndex++);
		});

		
	});
	
		var href = window.location.href;
		if(href.indexOf('?') == -1) {
			href = href + '?print=print';
		} else {
			href = href + '&print=print';
		}
		$('#printAnchor').attr('href',href);
	
		
// 		 Open the validation error popup when button clicked
	$("#pageErrorOpenerButton").click( function(){
		$("#errorPop").dialog({
				buttons : { "Close" : { text: "Close",
						         		  id: "errorPopClose",
						               click: function(){
						        	           $( "#errorPop" ).dialog( "close" );
											}   
				      				} 
						  }
				});
				$("#pageErrorDiv").show();
	           }
     );
	</script>
	</form>



	
