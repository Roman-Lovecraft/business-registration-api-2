	<!-- the EDIT entry address format -->
	<script>
	$(function() {	
		
 		(function ($) {
 		    $.fn.initAddressChk = function() {
		    		
		    	function toggleStates(e){
		    		showStates('busOverview_principalAddr');
		    		return false;
		    	} 
		    	
		    	populateStates();
		    	var selectCountryBox = '#busOverview_principalAddr_country';
		    	$(selectCountryBox + " option[value='blank']").remove();
		    	initStates('busOverview_principalAddr','INT','USA','OR');
		    	$('#busOverview_principalAddr_country').change(toggleStates);	
 		    	
 		    	$('#busOverview_principalAddr_addressLine1').on('keyup',addrKeyup);
 		    	$('#busOverview_principalAddr_city').on('keyup',addrFocusOut);
 		    	$('#busOverview_principalAddr_zip').on('keyup',addrFocusOut);
 		    	$('#busOverview_principalAddr_zip').on('focusout', function() {
 		    			$(this).trigger("change");	
 		    	});
 		    	function addrKeyup(e){
 		    		var addressLine1ValLength = $('#busOverview_principalAddr_addressLine1').val().length;
 		    		if (addressLine1ValLength > 6){
 		    			return addrFocusOut(e);
 		    		}
 		    	    return false;
 		    	}
 		    	
 		    	function addrFocusOut(e){
 		    		var countryVal = $('#busOverview_principalAddr_country').val();
 		    		if (countryVal == 'USA'){
 		    			validateAddressOnServer(e, 'busOverview_principalAddr', 'busOverview_principalAddr','true', 'INT');
 		    			//e.currentTarget.focus();
 		    		}
 		    	    return false;
 		    	}
 		    	
		    	$('#busOverview_principalAddr'+'ClearAddressHrefId').click(clearClicked);
		    	function clearClicked(e){
 		    		$('#busOverview_principalAddr_addressLine1').val('');
 		    		$('#busOverview_principalAddr_addressLine2').val('');
 		    		$('#busOverview_principalAddr_city').val('');
 		    		$('#busOverview_principalAddr_zip').val(''); 
 		    		showStates('busOverview_principalAddr', 'USA', 'OR');
 		    		$('#busOverview_principalAddr_extrnAddressListResult').val('0');
	 		    	$('#busOverview_principalAddr_addressLine1').focus();
	 		    	$('#busOverview_principalAddr_addressLine1').trigger('change');
		    		return false;
		    	} 
		    	
		    	busOverviewSectionObj.addIgnoredField('busOverview_principalAddr_altAddr_addressLine1');
		    	busOverviewSectionObj.addIgnoredField('busOverview_principalAddr_altAddr_addressLine2');
		    	busOverviewSectionObj.addIgnoredField('busOverview_principalAddr_altAddr_city');
		    	busOverviewSectionObj.addIgnoredField('busOverview_principalAddr_altAddr_state');
		    	busOverviewSectionObj.addIgnoredField('busOverview_principalAddr_altAddr_zip');
		    	busOverviewSectionObj.addIgnoredField('busOverview_principalAddr_altAddr_country');
		    	busOverviewSectionObj.addIgnoredField('busOverview_principalAddr_altAddr_errorcode');
		    	busOverviewSectionObj.addIgnoredField('busOverview_principalAddr_altAddr_errormessage');
 		    }
		 })(jQuery);
		 $('#busOverview_principalAddr').initAddressChk(); 		 
 	});
	</script>
