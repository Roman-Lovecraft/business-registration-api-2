	<!-- the EDIT entry address format -->
	<script>
	$(function() {	
		
 		(function ($) {
 		    $.fn.initAddressChk = function() {
		    		
		    	function toggleStates(e){
		    		showStates('jurisdiction_pplAddr');
		    		return false;
		    	} 
		    	
		    	populateStates();
		    	var selectCountryBox = '#jurisdiction_pplAddr_country';
		    	$(selectCountryBox + " option[value='blank']").remove();
		    	initStates('jurisdiction_pplAddr','INT','USA','OR');
		    	$('#jurisdiction_pplAddr_country').change(toggleStates);	
 		    	
 		    	$('#jurisdiction_pplAddr_addressLine1').on('keyup',addrKeyup);
 		    	$('#jurisdiction_pplAddr_city').on('keyup',addrFocusOut);
 		    	$('#jurisdiction_pplAddr_zip').on('keyup',addrFocusOut);
 		    	$('#jurisdiction_pplAddr_zip').on('focusout', function() {
 		    			$(this).trigger("change");	
 		    	});
 		    	function addrKeyup(e){
 		    		var addressLine1ValLength = $('#jurisdiction_pplAddr_addressLine1').val().length;
 		    		if (addressLine1ValLength > 6){
 		    			return addrFocusOut(e);
 		    		}
 		    	    return false;
 		    	}
 		    	
 		    	function addrFocusOut(e){
 		    		var countryVal = $('#jurisdiction_pplAddr_country').val();
 		    		if (countryVal == 'USA'){
 		    			validateAddressOnServer(e, 'jurisdiction_pplAddr', 'jurisdiction_pplAddr','true', 'INT');
 		    			//e.currentTarget.focus();
 		    		}
 		    	    return false;
 		    	}
 		    	
		    	$('#jurisdiction_pplAddr'+'ClearAddressHrefId').click(clearClicked);
		    	function clearClicked(e){
 		    		$('#jurisdiction_pplAddr_addressLine1').val('');
 		    		$('#jurisdiction_pplAddr_addressLine2').val('');
 		    		$('#jurisdiction_pplAddr_city').val('');
 		    		$('#jurisdiction_pplAddr_zip').val(''); 
 		    		showStates('jurisdiction_pplAddr', 'USA', 'OR');
 		    		$('#jurisdiction_pplAddr_extrnAddressListResult').val('0');
	 		    	$('#jurisdiction_pplAddr_addressLine1').focus();
	 		    	$('#jurisdiction_pplAddr_addressLine1').trigger('change');
		    		return false;
		    	} 
		    	
		    	jurisdictionSectionObj.addIgnoredField('jurisdiction_pplAddr_altAddr_addressLine1');
		    	jurisdictionSectionObj.addIgnoredField('jurisdiction_pplAddr_altAddr_addressLine2');
		    	jurisdictionSectionObj.addIgnoredField('jurisdiction_pplAddr_altAddr_city');
		    	jurisdictionSectionObj.addIgnoredField('jurisdiction_pplAddr_altAddr_state');
		    	jurisdictionSectionObj.addIgnoredField('jurisdiction_pplAddr_altAddr_zip');
		    	jurisdictionSectionObj.addIgnoredField('jurisdiction_pplAddr_altAddr_country');
		    	jurisdictionSectionObj.addIgnoredField('jurisdiction_pplAddr_altAddr_errorcode');
		    	jurisdictionSectionObj.addIgnoredField('jurisdiction_pplAddr_altAddr_errormessage');
 		    }
		 })(jQuery);
		 $('#jurisdiction_pplAddr').initAddressChk(); 		 
 	});
	</script>
