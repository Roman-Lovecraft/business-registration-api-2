	<!-- the EDIT entry address format -->
	<script>
	$(function() {	
		
 		(function ($) {
 		    $.fn.initAddressChk = function() {
		    		
		    	function toggleStates(e){
		    		showStates('memberManager_address');
		    		return false;
		    	} 
		    	
		    	populateStates();
		    	var selectCountryBox = '#memberManager_address_country';
		    	$(selectCountryBox + " option[value='blank']").remove();
		    	initStates('memberManager_address','INT','USA','OR');
		    	$('#memberManager_address_country').change(toggleStates);	
 		    	
 		    	$('#memberManager_address_addressLine1').on('keyup',addrKeyup);
 		    	$('#memberManager_address_city').on('keyup',addrFocusOut);
 		    	$('#memberManager_address_zip').on('keyup',addrFocusOut);
 		    	$('#memberManager_address_zip').on('focusout', function() {
 		    			$(this).trigger("change");	
 		    	});
 		    	function addrKeyup(e){
 		    		var addressLine1ValLength = $('#memberManager_address_addressLine1').val().length;
 		    		if (addressLine1ValLength > 6){
 		    			return addrFocusOut(e);
 		    		}
 		    	    return false;
 		    	}
 		    	
 		    	function addrFocusOut(e){
 		    		var countryVal = $('#memberManager_address_country').val();
 		    		if (countryVal == 'USA'){
 		    			validateAddressOnServer(e, 'memberManager_address', 'memberManager_address','true', 'INT');
 		    			//e.currentTarget.focus();
 		    		}
 		    	    return false;
 		    	}
 		    	
		    	$('#memberManager_address'+'ClearAddressHrefId').click(clearClicked);
		    	function clearClicked(e){
 		    		$('#memberManager_address_addressLine1').val('');
 		    		$('#memberManager_address_addressLine2').val('');
 		    		$('#memberManager_address_city').val('');
 		    		$('#memberManager_address_zip').val(''); 
 		    		showStates('memberManager_address', 'USA', 'OR');
 		    		$('#memberManager_address_extrnAddressListResult').val('0');
	 		    	$('#memberManager_address_addressLine1').focus();
	 		    	$('#memberManager_address_addressLine1').trigger('change');
		    		return false;
		    	} 
		    	
		    	memberManagerSectionObj.addIgnoredField('memberManager_address_altAddr_addressLine1');
		    	memberManagerSectionObj.addIgnoredField('memberManager_address_altAddr_addressLine2');
		    	memberManagerSectionObj.addIgnoredField('memberManager_address_altAddr_city');
		    	memberManagerSectionObj.addIgnoredField('memberManager_address_altAddr_state');
		    	memberManagerSectionObj.addIgnoredField('memberManager_address_altAddr_zip');
		    	memberManagerSectionObj.addIgnoredField('memberManager_address_altAddr_country');
		    	memberManagerSectionObj.addIgnoredField('memberManager_address_altAddr_errorcode');
		    	memberManagerSectionObj.addIgnoredField('memberManager_address_altAddr_errormessage');
 		    }
		 })(jQuery);
		 $('#memberManager_address').initAddressChk(); 		 
 	});
	</script>
