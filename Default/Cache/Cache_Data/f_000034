<script language="javascript" type="text/javascript">
	var busOverview_duration_dissolutionDate_input = $('#busOverview_duration_dissolutionDate');
	
	// if the durationDate is present then show the dissolution date
	if(busOverview_duration_dissolutionDate_input.val() != '') {
		$('#busOverview_duration_dissolutionDateDiv').removeClass('displayNone');
	}
	
	$('#busOverview_duration_type_perpetual').click(function() {
		// hide the div and clear the date
		$('#busOverview_duration_dissolutionDateDiv').addClass('displayNone');
		$('#busOverview_duration_dissolutionDate').val('');
	});
	
	$('#busOverview_duration_type_nonperpetual').click(function() {
		$('#busOverview_duration_dissolutionDateDiv').removeClass('displayNone');
	});
	
	
	// make the dissolution date input a jquery datepicker and check, if the dissolution date is in range for next year or not. 
	busOverview_duration_dissolutionDate_input.datepicker({
	    changeYear: true,
	    minDate: 0
	}).on('change', function(e) {
		
	     var dis_date = $(this).val();
		 var numOfYears = 1;
         
		 var todayDate = new Date(); 
         todayDate.setFullYear(todayDate.getFullYear() + numOfYears);
         todayDate.setDate(todayDate.getDate() -1);
          
         var dissolutionDate = new Date(dis_date);
         
         jQuery("#dialogCustomMessage").text("Are you sure you want your business to dissolve on " + dis_date + " ?");

 		if (todayDate > dissolutionDate){
 			$("#dialogBox").dialog("open");
		}else{
        	return true;
     	}
	});
	
	// Creation of Dialog box 
    $("#dialogBox").dialog({
        autoOpen: false,
        modal: true,
        resizable: true,
        height: "auto",
        width: "auto",
        draggable: true,
      });
    
    $("#dialogBox").dialog({
        buttons : {
          "Yes" : function() {
            $(this).dialog("close");
          },
          "No" : function() {
			$('#busOverview_duration_type_perpetual').trigger('click');
            $(this).dialog("close");
          }
        }
      });

	
 // set the correct radio as checked
 	$('input:radio[name="busOverview_duration_type"][value=""]').prop("checked", true);
	
</script>
				
			
