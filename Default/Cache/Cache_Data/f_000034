<script type="text/javascript">
	//set width of popup based on window size
// 				var popUpWidth;	
// 				function setPopUp(){
// 					if (window.innerWidth > 991){
// 						popUpWidth = '40vw';
// 					} else if (window.innerWidth > 767) {
// 						popUpWidth = '60vw';
// 					} else {
// 						popUpWidth = '95vw';
// 					}
// 				}	
// 				setPopUp();
	
	organizerSectionObj.saving = false;
	$('#editExistingName').val(false);
	organizerSectionObj.prevLength = organizerSectionObj.multipleObjectsArray.length;
	organizerSectionObj.multiObjectAddId = 'organizer_multiObjectAdd';
	organizerSectionObj.multiObjectTableId = 'organizer_multiAssocNameDisplayTable';
	
	organizerSectionObj.multiObjectPopupDiv = $('#organizer_addEditEntityDiv')
// 	.removeClass('displayNone')
	.createModalDialog(organizerSectionObj.uniqueSectionId,'Add/Edit Organizer', 
		[ { text: "Save", id: 'organizer'+"_saveButton", 
			click: function() { 
					$('#editExistingName').val(true);
					organizerSectionObj.getCbrPageObject().currentMultiObjAddEditPopupSaveButtonID = 'organizer'+'_saveButton';
					organizerSectionObj.getCbrPageObject().currentMultiObjAddEditPopupCancelButtonID = 'organizer'+'_cancelButton';
					organizerSectionObj.wasMultiObjUpdateDueToSaveButton = true;
					organizerSectionObj.update();
					// if the  multi object that is currently being edited is valid after updating, close this dialog box
					var occurrence = $('#organizer_assocNameOccurrence').val();
					var multiObject = organizerSectionObj.multipleObjectsArray[occurrence];
					if(multiObject != undefined && multiObject.valid) {
						organizerSectionObj.saving = true;
						if ('ORG'=='ORG') {
							organizer = $('#organizerErrorDiv');
							if(organizer != null) {
								organizer.hide();
							}
						}
						else if ('ORG'=='REG') {
								registrant = $('#regErrorDiv');
								if(registrant != null) {
									registrant.hide();
								}
						}
						else if ('ORG'=='INC') {
							incorporator = $('#incorporatorErrorDiv');
							if(incorporator != null) {
								incorporator.hide();
							}
						} else if ('ORG'=='IDK') {
							organizer = $('#indDirectKnowledgeErrorDiv');
							if(organizer != null) {
								organizer.hide();
							}
						}
						
						$(this).dialog('close');
//						$('#organizer_addEditEntityDiv').clearValues();
					}
				} 
			},
		  { text: "Cancel", id: 'organizer'+"_cancelButton", 
			click: function() { 
//				organizerSectionObj.restoreMultiObjectToPreviousState();
				$(this).dialog('close');
//				$('#organizer_addEditEntityDiv').clearValues();
				return false;
			} 
		  }
		],null,  responseWidth, 'auto', false);
	//Allows for both X and Cancel to reset state
	organizerSectionObj.multiObjectPopupDiv.bind('dialogclose', function(event) {
			if(!organizerSectionObj.saving || (organizerSectionObj.prevLength == 0 && organizerSectionObj.length == 1)) {
				organizerSectionObj.restoreMultiObjectToPreviousState();
				organizerSectionObj.prevLength = organizerSectionObj.multipleObjectsArray.length;
			}
//			$(this).dialog('close');
			$('#organizer_addEditEntityDiv').clearValues();
			$('#editExistingName').val(false);
			organizerSectionObj.saving = false;
			
			if(organizerSectionObj.multipleObjectsArray.length == 0) {
				
				$('#organizer_addEditEntityDiv').find('input[type=text],textarea').val('');
				
				$('#organizer_addEditEntityDiv').find('input[type=checkbox],input[type=radio]:checked').removeAttr('checked');
				
				$('#organizer_addEditEntityDiv').find('select').find('option').attr('selected',false);
				
				// clear assocName hidden fields
				$('#organizer_assocNameOccurrence')	.val('');
				$('#organizer_assocNameEntityType')	.val('');
				//$('#organizer_assocNameType')		.val('').change();
				if(organizerSectionObj.uniqueSectionId === 'indDirectKnowledge') {
					$('#' + organizerSectionObj.uniqueSectionId + '_assocNameType')		.val('IDK').change();
				} else if(organizerSectionObj.uniqueSectionId === 'incorporator') {
					$('#' + organizerSectionObj.uniqueSectionId + '_assocNameType')		.val('INC').change();
				} else if(organizerSectionObj.uniqueSectionId === 'reg') {
					$('#' + organizerSectionObj.uniqueSectionId + '_assocNameType')		.val('REG').change();
				} else if(organizerSectionObj.uniqueSectionId === 'organizer') {
					$('#' + organizerSectionObj.uniqueSectionId + '_assocNameType')		.val('ORG').change();
				} else {
					$('#' + organizerSectionObj.uniqueSectionId + '_assocNameType')		.val('').change();
				}
				
			} 
			
			
			
			return false;
	});
	
	
		
	organizerSectionObj.addReplacementValue('organizer_address_','address.');
	organizerSectionObj.addIgnoredField('organizer_address_extrnAddressListResult');
	organizerSectionObj.addIgnoredField('organizer_address_satoriResult');
	var requiredFieldId = "";
	if (false) {
		requiredFieldId = 'organizer_assocAuth_firstName';
	} else {
		requiredFieldId = 'organizer_individual_firstName';
	}
	
	organizerSectionObj.requiredFieldId = requiredFieldId;
	$("#organizer_addEditEntityDiv input[type='text']").change(function() {
		handleSpecialCharOnInput(this);
	});
</script>













			
