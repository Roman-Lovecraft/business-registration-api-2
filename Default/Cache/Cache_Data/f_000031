<script>
	var confirmDiv = $('<div class="standardDataDisplayStyle" />');
	confirmDiv
			.text('Deleting the business will permanently remove the business activity and all related data.');

	$(document).ready(function() {
		$('.busHomeActivityDelete').each(function() {
			$(this).click(function() {
				removeBusiness($(this).prop('href'));
				return false;
			});
		});

		$('#manageLicenses').click(function(event) {
			openLicensesDialog(event);
		});
	});
	var businessViewUrl = 'http://egov.sos.state.or.us/br/pkg_web_name_srch_inq.do_name_srch?p_name=&p_regist_nbr=';

	function openBusinessInBeri(registryNumber) {
		window.open(businessViewUrl + registryNumber);
	}
	function removeBusiness(href) {
		myPopupDiv = confirmDiv.createModalDialog('busHomeActivityDelete',
				'Delete Business', [ {
					text : "Delete Business Activity",
					id : "yesButton",
					click : function() {
						window.location.href = href;
						// close and remove the dialog
						$(this).remove();
					}
				}, {
					text : "Cancel",
					id : "noButton",
					click : function(e) {
						$(this).remove();
						e.stopImmediatePropagation();
					}
				} ], null, 450, 300);
	}

	function createEntityAndEntityAffomMapping(affomRsn) {
		var businessNameDialog = $('<div id="businessNameDialog" />');
		var label = $('<label for="businessNamePopupText" class="requiredFieldPromptStyle" value="Business Name:" >Business Name:</label>');
		var textBox = $('<input type="text" id="businessNamePopupText" class="standardEntryStyle" style="margin-left:20px;font-size:10pt;"/>');

		textBox.keypress(keystrokeValidation);
		textBox.change(function(event) {
			handleSpecialCharOnInputTextObjectArg(event.target);
			var field = $(this);
			field.val(field.val().toUpperCase());
		});
		textBox.keyup(businessNameLengthCheck);

		businessNameDialog.append(label);
		businessNameDialog.append(textBox);

		var businessNamePopupDiv = businessNameDialog
				.createModalDialog(
						'businessNameDialog',
						'Business Name',
						[
								{
									text : "Continue",
									id : "createEntityButton",
									click : function() {
										var businessName = textBox.val();
										if (businessName.trim().length > 0) {
											$
													.ajax({
														type : "POST",
														url : "globalAjax/createEntityAndEntityAffomMapping.action",
														data : {
															affomRsn : affomRsn,
															businessName : businessName
																	.trim()
														},
														beforeSend : function() {
															blockUI();
														},
														success : function(data) {
															if (data.entityRsn != null) {
																entityRsn = data.entityRsn;
															}
															unblockUI();
														} // end success method
													}); // end ajax 
										}
										businessNamePopupDiv.dialog('close');
										return false;
									}
								}, {
									text : "Cancel",
									id : "noEntityButton",
									click : function(e) {
										$(this).remove();
										e.stopImmediatePropagation();
									}
								} ], null, responseWidth, 300);
	}
</script>

