<script src="https://secure.sos.state.or.us/cbrstatic/js/objects/CbrObjectFunctions.js?timestamp=0307022025" type="text/javascript"></script>

<script type="text/javascript">
var licensesDialogContent = $('#licencesInfo');
var licensesDialog = $('<div id="licensesDialog" />');

var licensesPreTextParagraphVar = $('#licensesPreTextParagraph');
licensesPreTextParagraphVar.html(licensesPreTextValue);
$('#licensesPostTextParagraph').html(licensesPostTextValue);

var licenseCancelButtonIdVar = 'licenseCancelButton';
licensesDialogContent.removeClass("displayNone");
licensesDialog.append(licensesDialogContent);

//adjust width of dialog based on screen width
	var wWidth = $(window).width();
	var responseWidth;
	if (wWidth > 800){
	 	responseWidth = '30%'
	} else {
	 	responseWidth = '80%'
	}

// this is where the POPUP is defined
var licensesPopupDiv = licensesDialog.createModalDialog('licensesDialog', licensesPopupTitle, 
	[{ text: licensesCloseDialogValue, id: licenseCancelButtonIdVar, 
		click: function() { 
			$(this).dialog('close');
			unblockUI();
			return false;
		} 
	}]
	, null, responseWidth, 'auto', false);

// add the ajax for the button to open the partner site
$('.partner-redirect-button').each(function(){
	$(this).click(function(){
		var affomRsn = $('#'+this.id+'_affom').val();
		var redirectLink = $('#'+this.id+'_link').val();
		if (entityRsn == '') {
// 			createEntityAndEntityAffomMapping(affomRsn);
			window.open(redirectLink, "_blank");
			return false;
		} else {
			$.ajax({
			    type: "POST",
			    url: "globalAjax/createAffommSrd.action",
			    data: {affomRsn:affomRsn,entityRsn:entityRsn},
				beforeSend: function() {
					blockUI();
				},
			    success: function(data) {
					unblockUI();
				}	// end success method
			});	// end ajax 
			window.open(redirectLink, "_blank");
			licensesPopupDiv.dialog('close');
			return false;
		}
	});
});

// get a jquery reference to the cancel button
var licenseCancelButtonElementVar = $('#' + licenseCancelButtonIdVar);
// change the popup div buttons to be on the left instead of the right
licenseCancelButtonElementVar.parent().css('float','left');
// move the licenses <ul> element to be just after the preText
$('#licensesULElement').insertAfter(licensesPreTextParagraphVar);

function openLicensesDialog(event){
	if(event != undefined) {
		event.preventDefault();
	}
	licensesPopupDiv.dialog('open');
}
</script>

		</div>
