			$("#buttonBarDiv").removeClass('displayNone');
	});
	

	
		$(function() {
			$('#offlineOnlineRenewal').click(function() {
				var offlineOnlineRenewalDiv = $('<div class="standardDataDisplayStyle" />');
				var offlineLoginRenewalText = 'You may renew or reinstate your business with or without '
				                            + 'logging into the Oregon Business Registry.&nbsp;&nbsp;'
				                            + 'If you log in, the process is simplified and your data '
				                            + 'entry will be saved between sessions.&nbsp;&nbsp;'
										    + '<span style="font-weight: bold">We recommend logging in '
										    + 'to perform this task.</span><br/><br/>'
										    + 'If you already have a user account you can use it, '
										    + 'otherwise you can create a new user.';
				offlineOnlineRenewalDiv.html(offlineLoginRenewalText);
				offlineOnlineRenewalDiv.createModalDialog(
					'offlineOnlineRenewalModalDialog',
					'Log In to Renew/Reinstate',
					   [{	text : "Log in",
							id : "loginButton",
							click : function() {
								blockUI();
								$(this).remove();
								window.location = 'https://secure.sos.state.or.us/cbrbr/changeAppRenewal.action';
							}
						},
						{	text : "New User",
							id : "newUserButton",
							click : function() {
								blockUI();
								$(this).remove();
								window.location = 'https://secure.sos.state.or.us/oim/login?oimAction=signUpGo&p_a_cd=CBR&p_fp_gurl=https://secure.sos.state.or.us/cbrmanager/login.action';
							}
						},
						{	text : "Proceed without Log in",
							id : "renewWithoutLoginButton",
							click : function() {
								blockUI();
								$(this).remove();
								window.location = 'https://secure.sos.state.or.us/cbrbr/renewal.action';
							}
						}, 
						{	text : "Cancel",
							id : "cancelButton",
							click : function() {
								$(this).remove();
							}
						} ], null, responseWidth,
					'auto');
				});
			
			
			$("#enoticeUpdateDialog").dialog({
			    autoOpen  : false,
			    modal     : true,
			    title     : "Enter your Registry Number",
			    responsive: true,
			    width: 500,
			    height: 270,   
			    buttons   : {
			              'Update' : function() {
			                  var registryNbr = $('#registryNumber').val();
			                  if (registryNbr.length>0){
			                	   registryNbr = registryNbr.replace(/[^\w\s]/gi,'');
  			                	   var regWithNoAlphabets = /^\d+$/.test(registryNbr);
			                	  
			                	  if (regWithNoAlphabets == true){
			                		  blockUI();
			                			$(this).remove();
										  window.location = 'https://secure.sos.state.or.us/cbrbr/enotice.action?status=update&registryNumber='+registryNbr;;
									  $(this).remove();
			                		}else{
			                			 $(".entryErrorStyle").show();
			                		}
			                	 }			                  
			                  else{
			                	  $(".entryErrorStyle").show();
			                  }
			              },
			              'Close' : function() {
			                  $(this).dialog('close');
			              }
			                }
			});
			
			function numberOnly() {
			    var element = document.getElementById(registryNumber);
			    var regex = /[^0-9]/gi;
			    element.value = element.value.replace(regex, "");
			}
						
			$('#enoticeButton').click(function() {
				var enoticeButtonDiv = $('<div class="standardDataDisplayStyle" />');
				var enoticeButtonText = '<b>You will be notified by the Secretary of State when it is time to renew. <br/>'
										+'Select one of the following notification options by clicking the button below :</b></br></br>'
