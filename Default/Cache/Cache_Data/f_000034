<script type="text/javascript">

	professionalServicesSectionObj.requiredFieldId = 'professionalServices_trueType';
	
	var professionalServicesTypeDiv = $('#professionalServicesType');

	// hide the professionalServicesTypeDiv at the start and show when true is clicked
	professionalServicesTypeDiv.hide();
	if(''=='true') {
		$('#professionalServices_trueType').click();
		professionalServicesTypeDiv.show();
	} else if(''=='false'){
		$('#professionalServices_falseType').click();
	}

	function handleServicesTypeChange(event) {
		var servicesSelected = $('#professionalServices_trueType').val();
		var val = $('#professionalServicesSelection input[type="radio"]:checked').val();
		if (val == "true") {
			professionalServicesTypeDiv.show();
		} else {
			professionalServicesTypeDiv.hide();
		}
	}

	$(function($) {
		$('#professionalServicesSelection input:radio').change(handleServicesTypeChange);
	});
</script>


			
			
			
















 



