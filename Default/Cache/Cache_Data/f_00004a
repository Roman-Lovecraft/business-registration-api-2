<!-- MAIN CONTENT -->
<div id="main" class="container-fluid d-block mt-3 pt-0 pl-3 pr-3 pl-md-5 pr-md-5 pb-3">
	
	
	<input type="hidden" name="signatureType" value="dynamicDomestic" id="saveSignature_signatureType"/>
	<input type="hidden" name="activityTypeCode" value="REG" id="saveSignature_activityTypeCode"/>

	<script type="text/javascript">
		var cbrPageObj = new CbrPage();
		var count = 0;	
		var signatureNamePromptId = 'signatureNamePrompt';
		var signatureNameSpanId = 'signatureNameSpan';
		var signatureTitleInputId = 'signatureTitle';
		var dynamicDomesticSelectSpanId = 'dynamicDomesticSelectSpan';
		var signatureNameInputId = 'signatureName';
		var signatureNameHiddenInputId = 'signatureNameHidden';
		//adjust width of dialog based on screen width
			var wWidth = $(window).width();
			var responseWidth;
			if (wWidth > 800){
			 	responseWidth = '30%'
			} else {
			 	responseWidth = '80%'
			}
		
		$(function() {
			$('#pageButton1')
				.find('span').text('Submit').end()
				.click(function(e) {
					var errorMessage = '';
					var errorTitle = 'Missing Data';	// default title
					var clientErrorDiv = $('<div class="standardDataDisplayStyle" />');
					// for each signature check that the checkbox is checked
					for(var i = 0; i < count; i++) {
						// rows could have been deleted, if the element doesn't exist, skip and go to the next
						if($('#' + signatureTitleInputId + i).length == 0) {
							continue;
						}
						
						var title 	= $.trim($('#' + signatureTitleInputId + i).val());
						var name 	= $.trim($('#' + signatureNameInputId + i).val());
						var checkbox = $('#signatureCheckBox' + i).prop('checked');
						
						if(title == '') {
							errorMessage = 'All signatures must have a title.';
							break;	// error found, exit the for loop
						} else if(name == '') {
							errorMessage = 'All signatures must have a name.';
							break;	// error found, exit the for loop
						} else if(title != '' && name != '' && !checkbox) {
							errorMessage = 'You must click the Sign checkbox for ' + title + ' ' + name + '.';
							errorTitle = 'Forgot to Sign';
							break;	// error found, exit the for loop
						}
					}
					
					// if an error message was set then show the modal ok dialog box, otherwise submit the form
					if(errorMessage == '') {
						// no errors were found, submit the form
						$(this).closest("form").submit();
					} else {
						clientErrorDiv.text(errorMessage);
						clientErrorDiv.createModalOkDialog('requiredFieldsNotMetDiv',errorTitle,'position',responseWidth,'auto');
						e.stopImmediatePropagation();	// this stops any subsequent events such as blockUI div
						return false;	// this stops any subsequent events such as blockUI div
					}
			});
			
			var entityType = 'Dllc';
			var activityTypeHidden = $('#saveSignature_activityTypeCode');
			
			var activityType = activityTypeHidden.val() == 'RNWL'? 'renewal':
				activityTypeHidden.val() == 'RINS'? 'reinstat': activityTypeHidden.val() == 'REAC'? 'reactive': 
			    activityTypeHidden.val() == 'OIC'? 'oic':'registration';
			
			var reviewUrl = 'https://secure.sos.state.or.us/cbrbr/review_' + activityType + '_' + entityType + '.action';
			
			$('#pageButton2')
				.find('span').text('Return to Review').end()				
				.attr('href',reviewUrl);
			
			$('#pageButton3')
				.find('span').text('Add Signer').end()			
				.click(function(event) {
					event.preventDefault();
					createSignatureRow(null,null,false);
			});
			
			
			// show the buttonBarDiv
			$('#buttonBarDiv').removeClass('displayNone');	
			// move the editButtonBarDiv inside the buttonBarDiv
			$('#editButtonBarDiv').appendTo('#buttonBarDiv');
			
			
		});
		
		
		var titlesArray = new Array();
		
		titlesArray.push({displayValue:'Authorized Agent'});
		
		titlesArray.push({displayValue:'Organizer'});
		
		
		
		function createSignatureRow(title, name, signed) {
			// get the rows from the signature table
			var rows = $('#signatureTable').children('tbody').children('tr');
			
			// get the signature row div 
			var signRow = $( "#signRow" );
			
			//create divs
			var titleDiv = $('<div id="title_' + count + '" class="col-12 col-md-4  mt-3">');
			var nameDiv = $('<div id="name_' + count + '" class="col-12 col-md-4  mt-3">');
			var signDiv = $('<div id="sign_' + count + '" class="col-12 col-md-4  mt-3 mt-md-0 align-self-center">');		
			
			// create row1
			var signatureRow1 = $('<tr id="signatureRow1_' + count + '" style="vertical-align:middle;">');

			// add cells to the row
			signatureRow1.append('<td class="requiredFieldPromptStyle smallText" style="width:47px">Title</td>');
				
				//dynamic signature
				
					
						
						signatureRow1.append('<td style="width:169px"><input type="text" id="' + signatureTitleInputId + count + '" name="signatures[' + count + '].title" class="standardEntryStyle smallText" /></td>').append('<td style="width:117px"><a href="#" id="selectTitleHref' + count + '" class="actionPromptStyle">Select</a></td>');
						
						//add title to div
						titleDiv.append('Title');
						titleDiv.append('<input type="text" id="' + signatureTitleInputId + count + '" name="signatures[' + count + '].title" class="standardEntryStyle form-control" />');								
						titleDiv.append('<a href="#" id="selectTitleHref' + count + '" class="actionPromptStyle">Select</a></div></div>')
					
					
				
				
				//fixed signature
				
				
				signatureRow1.append('<td class="requiredFieldPromptStyle smallText" style="width:80px"><span id="' + signatureNamePromptId + count + '">Name</span></td>');
				
				//name title
				nameDiv.append('<span id="' + signatureNamePromptId + count + '" class="requiredFieldPromptStyle">Name</span>' + '</br>');
				nameDiv.append(name);
							
				
					var signatureNameTd = $('<td>');
					var signatureNameSpan = $('<span id="'+ signatureNameSpanId + count + '">');
					signatureNameTd.append(signatureNameSpan);
					signatureNameSpan.append('<input type="hidden" id="' + signatureNameHiddenInputId + count + '" name="signatures[' + count + '].name" />')
					signatureNameSpan.append('<input type="text" id="' + signatureNameInputId + count + '" class="standardEntryStyle form-control" />')
					
					//add name to div
					nameDiv.append(signatureNameSpan);
				
					
				   	if (cbrPageObj.individualArray.length > 0){
				   		signatureNameSpan.append(
							'<a href="#" id="selectNameHref' + count + '" class="actionPromptStyle">Select from List</a>' + 
							'<span class="actionPromptStyle">&nbsp;|&nbsp;</span>' + 
							'<a href="#" id="clearNameHref' + count + '" class="actionPromptStyle">Clear Name</a>'							
				        );
				       
				  	} else {
				  		signatureNameSpan.append('<a href="#" id="clearNameHref' + count + '" class="actionPromptStyle">Clear Name</a>');
				   	}
					signatureRow1.append(signatureNameTd);
				
				
				
			// create row2
			var signatureRow2 = $('<tr id="signatureRow2_' + count + '" style="vertical-align:middle;">');
			
			// add cells to the row
			signatureRow2
				.append('<td></td>')
				.append(
					'<td>' +
						'<input id="signatureCheckBox' + count + '" name="" type="checkbox" style="margin-left: 2em; margin-right: 1em;" />' +
						'<input id="signatureCheckBoxHidden' + count + '" name="signatures[' + count + '].signed" type="hidden" />' +
						'<label for="signatureCheckBox' + count + '" class="requiredFieldPromptStyle smallText">Sign</label>' +
					'</td>'
				)
				.append(
					'<td colspan="4">' +
					'<div id="signatureDisplayDiv' + count + '" class="signatureStyle mediumText"></div>' + 
					'</td>'
				);
				
				//add signature to sign div
				signDiv.append('<input id="signatureCheckBox' + count + '" name="" type="checkbox" style="form-control" />');
				signDiv.append('<input id="signatureCheckBoxHidden' + count + '" name="signatures[' + count + '].signed" type="hidden" style="form-control" />');
				signDiv.append('<label for="signatureCheckBox' + count + '" class="ml-1 requiredFieldPromptStyle">Sign</label>');
				signDiv.append('<span id="signatureDisplayDiv' + count + '" class="ml-2 mr-2 signatureStyle"></span>');
			
			// add the created row to the signatureTable
			// NOTE: this has to be added to the page dom before the elements can be looked up via jquery
			$('#signatureTable').append(signatureRow1);
			$('#signatureTable').append(signatureRow2);
			
			//add columns to the row
			signRow.append(titleDiv);
			signRow.append(nameDiv);
			signRow.append(signDiv);
			
			$('#' + signatureTitleInputId + count).keypress(keystrokeValidation);

			
			// function to remove the name value for this row
			$('#clearNameHref' + count).click({count:count},function(event){
				$('#' + signatureNameInputId + event.data.count).val('').change();
			});
			// function to open the popup for name
			$('#selectNameHref' + count).click({count:count},function(event){
				selectPopup('' + signatureNameInputId + event.data.count,this.id,cbrPageObj.individualArray,'cbrPageObj.individualArray','handleSelectedSingleName');
				return false;
			});
			// function to open the popup for title
			$('#selectTitleHref' + count).click({count:count},function(event){
				selectPopup('' + signatureTitleInputId + event.data.count,this.id,titlesArray,'titlesArray','handleSelectedTitle');
				return false;
			});
			
			$('#' + signatureNameInputId + count).keypress(keystrokeValidation);
			
			// function to clear displayed signature
			$('#' + signatureTitleInputId + count).change({count:count},clearSignedValue);
			$('#' + signatureNameInputId + count).change({count:count},clearSignedValue);

			// function to validate the length of the fields
			$('#' + signatureTitleInputId + count).change({count:count},function(event) {
				var obj = $('#' + signatureTitleInputId + event.data.count);
				var value = obj.val().trim();
				var maxLength = 32;
				if(value.length > maxLength) {
					var messageDiv = $('<div>');
					messageDiv.html('Title [' + value + '] exceeds maximum length of ' + maxLength);
					messageDiv.createModalOkDialog('popup' + signatureTitleInputId + count, 'Max Length Exceeded');
					var shortenedString = value.substring(0,maxLength);
					obj.val(shortenedString);
				}
			});
			$('#' + signatureNameInputId + count).change({count:count},function(event) {
				var obj = $('#' + signatureNameInputId + event.data.count);
				var value = obj.val().trim();
				var maxLength = 108;
				if(value.length > maxLength) {
					var messageDiv = $('<div>');
					messageDiv.html('Name [' + value + '] exceeds maximum length of ' + maxLength);
					messageDiv.createModalOkDialog('popup' + signatureTitleInputId + count, 'Max Length Exceeded');
					var shortenedString = value.substring(0,maxLength);
					obj.val(shortenedString);
				}
			});

			

			
			// create the elements for each row and hide them.
			// then show them when there is a matching signature title
			
			var dynamicDomesticTitleId = 'signatureDynamicDomesticTitlePrompt';
			var dynamicDomesticSignerTitleId = 'signatureDynamicDomesticSignerTitlePrompt';
			
			var signatureTitleObj = $('#' + signatureTitleInputId + count);

			// create and hide the prompts
			$('#' + signatureNamePromptId + count).before($('<span id="' + dynamicDomesticTitleId + count + '" class="requiredFieldPromptStyle smallText">Organizer</span>').hide());
			$('#' + dynamicDomesticTitleId + count).after($('<div id="' + dynamicDomesticSignerTitleId + count + '" class="requiredFieldPromptStyle smallText" style="margin-top: 7px;">Signer</div>').hide());

			// copy and change the id of the select html element
			$('#' + signatureNameSpanId + count).before($('<span id="' + dynamicDomesticSelectSpanId + count + '"></span>').hide().append($('#dynamicDomesticSelect').clone().attr('id','dynamicDomesticSelect' + count)).append('<br/>'));
			
			function hideNameFields(count, value) {
				if('true' == value) {
					// if true then it is an individual and we already have the name
					$('#' + signatureNamePromptId + count).hide();
					$('#' + signatureNameSpanId + count).hide();
					$('#' + dynamicDomesticSignerTitleId + count).hide();
					// set the input field as the dropdown name
					var name = $('#dynamicDomesticSelect' + count + ' option:selected').text();
					$('#' + signatureNameInputId + count).val(name);
				} else {
					// if false then its a bofr or bnofr and we need to gather the name
					$('#' + signatureNamePromptId + count).hide();
					$('#' + dynamicDomesticSignerTitleId + count).show();
					$('#' + signatureNameSpanId + count).show();
					// clear the value from the name input field
					$('#' + signatureNameInputId + count).val('');
				}
			}
			
			// assign a change function to handle future changes
			$('#dynamicDomesticSelect' + count).change({count:count},function(event) {
				var count = event.data.count;
				var value = $(this).val();
				hideNameFields(count,value);
			});
			
			function enteredDynamicDomesticValue(count) {
				return $('#' + signatureTitleInputId + count).val().toLowerCase() == 'Organizer'.toLowerCase();
			}
			
			signatureTitleObj.change({count:count},function(event) {
				var count = event.data.count;
				if(enteredDynamicDomesticValue(count)) {
					// show the dynamicDomestic elements
					$('#' + dynamicDomesticTitleId + count).show();	
					$('#' + dynamicDomesticSelectSpanId + count).show();	
					
					// hide the default elements
					$('#' + signatureNamePromptId + count).hide();
					$('#' + signatureNameSpanId + count).hide();

// 					run the function one time to handle the current value of the select list
// 					hideNameFields(count,$('#dynamicDomesticSelect' + count).val());
				} else {
					// hide the dynamicDomestic elements
					$('#' + dynamicDomesticTitleId + count).hide();
					$('#' + dynamicDomesticSelectSpanId + count).hide();
					$('#' + dynamicDomesticSignerTitleId + count).hide();
					
					// show the default elements
					$('#' + signatureNamePromptId + count).show();
					$('#' + signatureNameSpanId + count).show();
					$('#' + signatureNameInputId + count).val('');				
				}
			});
			

			// function to display signature when checked
			$('#signatureCheckBox' + count).click({count:count},function(event){
				var myCount = event.data.count;
				var signatureDisplayDiv = $('#signatureDisplayDiv' + myCount);
				var checkedHidden = $('#signatureCheckBoxHidden' + myCount);

				if(this.checked) {
					// validate that the title and name fields have values
					var titleValue 	= $.trim($('#' + signatureTitleInputId + myCount).val());
					var nameValue 	= $.trim($('#' + signatureNameInputId + myCount).val());
					
					if(titleValue == undefined || titleValue.length == 0 ||	nameValue == undefined || nameValue.length == 0) {
						var requiredFieldsNotMetDiv = $('<div class="standardDataDisplayStyle" />');
						requiredFieldsNotMetDiv.text('You must enter a Name and Title before clicking the Sign checkbox.');
						requiredFieldsNotMetDiv.createModalOkDialog('requiredFieldsNotMetDiv','Required Data Missing');
						this.checked = false;
						checkedHidden.val(false);
					} else {
						
						if(enteredDynamicDomesticValue(myCount) && $('#dynamicDomesticSelect' + myCount).val() == 'false') {
							var busName = $('#dynamicDomesticSelect' + myCount + ' option:selected').text();
							nameValue += ' (' + busName + ')';
						}
						
						signatureDisplayDiv.text(nameValue);
						$('#' + signatureNameHiddenInputId + myCount).val(nameValue);
						checkedHidden.val(true);
					}
				} else {
					signatureDisplayDiv.text('');
					checkedHidden.val(false);
				}					
			});
			
			
			if(count > 0) {
				// for all rows except the first, supply a delete button to remove the row
				//signatureRow1.append($('<td style="width:85px">').append($('#deleteSignatureHref').clone().attr('id','deleteSignatureHref' + count)));
				signDiv.append($('#deleteSignatureHref').clone().attr('id','deleteSignatureHref' + count));
				$('#deleteSignatureHref' + count).click({count:count},function(event){
					var myCount = event.data.count;
					var confirmDiv = $('<div class="standardDataDisplayStyle" />');
					confirmDiv.text('Do you wish to delete this signature section (you can add it again later if you choose)?');
					confirmDiv.createModalDialog('signatureDeleteConfirm','Delete Signature', 
						[ { text: "Yes", id: "yesButton", 
							click: function() { 
									$('#signatureRow1_' + myCount).remove();		
									$('#signatureRow2_' + myCount).remove();
									
									//remove signature row divs
									$('#title_' + myCount).remove();
									$('#name_' + myCount).remove();
									$('#sign_' + myCount).remove();	
									
									// repaint table's greenbar coloring
									paintTableGreenBar('signatureTable',2);
									
									// close and remove the dialog
					                $(this).remove(); 
									} 
							},
						  { text: "No", id: "noButton", 
							click: function() { 
										$(this).remove(); 
									} 
						}],'position', responseWidth, '600');
				});
			} else {
				signatureRow1.append('<td width="7%"></td>');
			}
			
			

			// set values if passed in
			if(title != '') {
				$('#' + signatureTitleInputId + count).val(title);
			}
			if(name != '') {
				$('#' + signatureNameInputId + count).val(name);
			}
			if(signed) {
				// set the checkbox to checked, execute the click function and recheck the checkbox (executing the click function unchecks the box)
				$('#signatureCheckBox' + count).prop("checked",true);
			}
			
			// repaint table's greenbar coloring
			paintTableGreenBar('signatureTable',2);
			
			

			// lastly, increment the count var
			count++;
		}
		
		function handleSelectedTitle(uniqueId, selectedTitle) {
			$('#' + uniqueId).val(selectedTitle.displayValue).change();
		}
		
		function clearSignedValue(event) {
			var count = event.data.count;
			var checkbox = $('#signatureCheckBox' + count);
			if(checkbox.is(':checked')){
				// uncheck the checkbox, execute the click function and re-uncheck the checkbox (executing the click function checks the box)
				checkbox.prop("checked",false);
				// clear the displayed signature
				$('#signatureDisplayDiv' + count).text('');
			}
		}
		
		$(function(){
		
			// if no values found, create the first row
			createSignatureRow(null,null,false);
		
		
			
			// show error message if present
		
		getIpInfo();
		});
		
		
		// pick list for name field
		cbrPageObj.addIndividual(Individual.createFromView(jQuery.parseJSON('{\"firstName\":\"John\",\"middleName\":\"\",\"lastName\":\"Doe\",\"suffixName\":\"\",\"concatenatedName\":\"John Doe\",\"beriCode\":\"\",\"firstNameberiMessage\":\"\",\"secondNameberiMessage\":\"\"}')));cbrPageObj.addIndividual(Individual.createFromView(jQuery.parseJSON('{\"firstName\":\"John\",\"middleName\":\"\",\"lastName\":\"Doe\",\"suffixName\":\"\",\"concatenatedName\":\"John Doe\",\"beriCode\":\"\",\"firstNameberiMessage\":\"\",\"secondNameberiMessage\":\"\"}')));cbrPageObj.addIndividual(Individual.createFromView(jQuery.parseJSON('{\"firstName\":\"John\",\"middleName\":\"Michael\",\"lastName\":\"Doe\",\"suffixName\":\"\",\"concatenatedName\":\"John Michael Doe\",\"beriCode\":\"\",\"firstNameberiMessage\":\"\",\"secondNameberiMessage\":\"\"}')));
		
	</script>
	
		<div class="col-12 pl-0"><h1>Signatures</h1></div>
		
		<div class="col-12 pl-0 ">
			I declare, under penalty of perjury, that this document does not fraudulently conceal, fraudulently obscure, fraudulently alter or otherwise misrepresent the identity of the person or any officers, managers, members or agents of the limited liability company on behalf of which the person signs. This filing has been examined by me and is, to the best of my knowledge and belief, true, correct, and complete. Making false statements in this document is against the law and may be penalized by fines, imprisonment, or both.
			<br/>
			<br/> 
			By typing my name in the electronic signature field, I am agreeing to conduct business electronically with the State of Oregon. I understand that transactions and/or signatures in records may not be denied legal effect solely because they are conducted, executed, or prepared in electronic form and that if a law requires a record or signature to be in writing, an electronic record or signature satisfies that requirement.
		</div>
		
