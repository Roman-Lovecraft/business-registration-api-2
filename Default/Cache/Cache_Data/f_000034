<script type="text/javascript">
	//set width of popup based on window size
// 				var popUpWidth;	
// 				function setPopUp(){
// 					if (window.innerWidth > 991){
// 						popUpWidth = '40vw';
// 					} else if (window.innerWidth > 767) {
// 						popUpWidth = '60vw';
// 					} else {
// 						popUpWidth = '95vw';
// 					}
// 				}	
// 				setPopUp();
	
	indDirectKnowledgeSectionObj.saving = false;
	$('#editExistingName').val(false);
	indDirectKnowledgeSectionObj.prevLength = indDirectKnowledgeSectionObj.multipleObjectsArray.length;
	indDirectKnowledgeSectionObj.multiObjectAddId = 'indDirectKnowledge_multiObjectAdd';
	indDirectKnowledgeSectionObj.multiObjectTableId = 'indDirectKnowledge_multiAssocNameDisplayTable';
	
	indDirectKnowledgeSectionObj.multiObjectPopupDiv = $('#indDirectKnowledge_addEditEntityDiv')
// 	.removeClass('displayNone')
	.createModalDialog(indDirectKnowledgeSectionObj.uniqueSectionId,'Add/Edit Individual with Direct Knowledge', 
		[ { text: "Save", id: 'indDirectKnowledge'+"_saveButton", 
			click: function() { 
					$('#editExistingName').val(true);
					indDirectKnowledgeSectionObj.getCbrPageObject().currentMultiObjAddEditPopupSaveButtonID = 'indDirectKnowledge'+'_saveButton';
					indDirectKnowledgeSectionObj.getCbrPageObject().currentMultiObjAddEditPopupCancelButtonID = 'indDirectKnowledge'+'_cancelButton';
					indDirectKnowledgeSectionObj.wasMultiObjUpdateDueToSaveButton = true;
					indDirectKnowledgeSectionObj.update();
					// if the  multi object that is currently being edited is valid after updating, close this dialog box
					var occurrence = $('#indDirectKnowledge_assocNameOccurrence').val();
					var multiObject = indDirectKnowledgeSectionObj.multipleObjectsArray[occurrence];
					if(multiObject != undefined && multiObject.valid) {
						indDirectKnowledgeSectionObj.saving = true;
						if ('IDK'=='ORG') {
							organizer = $('#organizerErrorDiv');
							if(organizer != null) {
								organizer.hide();
							}
						}
						else if ('IDK'=='REG') {
								registrant = $('#regErrorDiv');
								if(registrant != null) {
									registrant.hide();
								}
						}
						else if ('IDK'=='INC') {
							incorporator = $('#incorporatorErrorDiv');
							if(incorporator != null) {
								incorporator.hide();
							}
						} else if ('IDK'=='IDK') {
							organizer = $('#indDirectKnowledgeErrorDiv');
							if(organizer != null) {
								organizer.hide();
							}
						}
						
						$(this).dialog('close');
//						$('#indDirectKnowledge_addEditEntityDiv').clearValues();
					}
				} 
			},
		  { text: "Cancel", id: 'indDirectKnowledge'+"_cancelButton", 
			click: function() { 
//				indDirectKnowledgeSectionObj.restoreMultiObjectToPreviousState();
				$(this).dialog('close');
//				$('#indDirectKnowledge_addEditEntityDiv').clearValues();
				return false;
			} 
		  }
		],null,  responseWidth, 'auto', false);
	//Allows for both X and Cancel to reset state
	indDirectKnowledgeSectionObj.multiObjectPopupDiv.bind('dialogclose', function(event) {
			if(!indDirectKnowledgeSectionObj.saving || (indDirectKnowledgeSectionObj.prevLength == 0 && indDirectKnowledgeSectionObj.length == 1)) {
				indDirectKnowledgeSectionObj.restoreMultiObjectToPreviousState();
				indDirectKnowledgeSectionObj.prevLength = indDirectKnowledgeSectionObj.multipleObjectsArray.length;
			}
//			$(this).dialog('close');
			$('#indDirectKnowledge_addEditEntityDiv').clearValues();
			$('#editExistingName').val(false);
			indDirectKnowledgeSectionObj.saving = false;
			
			if(indDirectKnowledgeSectionObj.multipleObjectsArray.length == 0) {
				
				$('#indDirectKnowledge_addEditEntityDiv').find('input[type=text],textarea').val('');
				
				$('#indDirectKnowledge_addEditEntityDiv').find('input[type=checkbox],input[type=radio]:checked').removeAttr('checked');
				
				$('#indDirectKnowledge_addEditEntityDiv').find('select').find('option').attr('selected',false);
				
				// clear assocName hidden fields
				$('#indDirectKnowledge_assocNameOccurrence')	.val('');
				$('#indDirectKnowledge_assocNameEntityType')	.val('');
				//$('#indDirectKnowledge_assocNameType')		.val('').change();
				if(indDirectKnowledgeSectionObj.uniqueSectionId === 'indDirectKnowledge') {
					$('#' + indDirectKnowledgeSectionObj.uniqueSectionId + '_assocNameType')		.val('IDK').change();
				} else if(indDirectKnowledgeSectionObj.uniqueSectionId === 'incorporator') {
					$('#' + indDirectKnowledgeSectionObj.uniqueSectionId + '_assocNameType')		.val('INC').change();
				} else if(indDirectKnowledgeSectionObj.uniqueSectionId === 'reg') {
					$('#' + indDirectKnowledgeSectionObj.uniqueSectionId + '_assocNameType')		.val('REG').change();
				} else if(indDirectKnowledgeSectionObj.uniqueSectionId === 'organizer') {
					$('#' + indDirectKnowledgeSectionObj.uniqueSectionId + '_assocNameType')		.val('ORG').change();
				} else {
					$('#' + indDirectKnowledgeSectionObj.uniqueSectionId + '_assocNameType')		.val('').change();
				}
				
			} 
			
			
			
			return false;
	});
	
	
		
	indDirectKnowledgeSectionObj.addReplacementValue('indDirectKnowledge_address_','address.');
	indDirectKnowledgeSectionObj.addIgnoredField('indDirectKnowledge_address_extrnAddressListResult');
	indDirectKnowledgeSectionObj.addIgnoredField('indDirectKnowledge_address_satoriResult');
	var requiredFieldId = "";
	if (false) {
		requiredFieldId = 'indDirectKnowledge_assocAuth_firstName';
	} else {
		requiredFieldId = 'indDirectKnowledge_individual_firstName';
	}
	
	indDirectKnowledgeSectionObj.requiredFieldId = requiredFieldId;
	$("#indDirectKnowledge_addEditEntityDiv input[type='text']").change(function() {
		handleSpecialCharOnInput(this);
	});
</script>
















 


			
			

			
			
			
















 



